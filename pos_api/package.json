{
  "name": "custom-business-logic",
  "version": "1.0.0",
  "description": "Custom GraphQL API for PSA advisor applications",
  "main": "build/start.js",
  "scripts": {
    "test": "env NODE_ENV=test jest --coverage",
    "test:watch": "env NODE_ENV=test jest --coverage --watchAll",
    "eslint": "eslint . --fix --ext .ts",
    "start-dev": "ts-node-dev --respawn src/boot.ts",
    "build": "rm -rf ./build && tsc",
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "start": "node dist/boot.js",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "filter": "npm run eslint && npm run prettier:write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run filter"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/petra-sales-agency/custom-business-logic.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/petra-sales-agency/custom-business-logic/issues"
  },
  "homepage": "https://gitlab.com/petra-sales-agency/custom-business-logic#readme",
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/convict": "^4.2.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/ejs": "^3.0.4",
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.4",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/node": "^12.12.22",
    "@types/node-fetch": "^2.5.4",
    "@types/qs": "^6.9.3",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^3.1.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "@types/puppeteer": "^3.0.1",
    "apollo-server-express": "^2.9.15",
    "aws-sdk": "^2.657.0",
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "camelize": "^1.0.0",
    "class-validator": "^0.11.0",
    "convict": "^5.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphqurl": "^0.3.3",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "qs": "^6.9.4",
    "reflect-metadata": "^0.1.13",
    "save-dev": "0.0.1-security",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "winston": "^3.2.1"
  }
}
